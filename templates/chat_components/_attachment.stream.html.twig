{% if message.attachmentsIds is defined %}
    <div class="row px-3 pt-1">
        {% for id in message.attachmentsIds %}
            <div class="col-6 px-0">
                <img
                src="{{ path(getRouteName('APP_GET_SENT_IMG'), {attachmentId: id}) }}"
                class="img-thumbnail img-fluid">
            </div>
        {% endfor %}
    </div>
{% endif %}